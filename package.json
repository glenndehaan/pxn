{
  "name": "pxn",
  "version": "0.0.0",
  "description": "Plex XMLTV Netherlands",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --watch src --exec 'npm run lint && node ./src/index.js --'",
    "lint": "eslint -c ./package.json ./"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "commander": "^5.1.0",
    "deepmerge": "^4.2.2",
    "node-fetch": "^2.6.0"
  }
}
